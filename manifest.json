{
  "name": "JeN",
  "short_name": "JeN",
  "description": "Расширение для слежения за веткой.",
  "version": "1.2.0",
  "content_scripts": [
    {
      "matches": [
        "http://ci-platform.sbis.ru/*",
        "https://*.sbis.ru/*"
      ],
      "js": [
        "dist/index.js"
      ]
    }
  ],

  "icons": {
    "36": "images/icon.png",
    "48": "images/icon-48.png"
  },
  "action": {
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [],
  "host_permissions": [
    "http://ci-platform.sbis.ru/",
    "https://online.sbis.ru/"
  ],
  "manifest_version": 3
}
